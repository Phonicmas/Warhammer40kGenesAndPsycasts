<Defs>

	<!-- Inviolable Flesh -->

	<HediffDef ParentName="PsycastHediffBase">
		<defName>BEWH_InviolableFlesh</defName>
		<label>inviolable flesh</label>
		<description>Crusted with filth and foulness, you're more able to withstand attacks.</description>
		<isBad>false</isBad>
		<stages>
			<li>
				<statFactors>
					<MaxHitPoints>1.35</MaxHitPoints>
					<Beauty>0.1</Beauty>
					<FilthRate>3</FilthRate>
					<IncomingDamageFactor>0.8</IncomingDamageFactor>
				</statFactors>
			</li>
		</stages>
	</HediffDef>

