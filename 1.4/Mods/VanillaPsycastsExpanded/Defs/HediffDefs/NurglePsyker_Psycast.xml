	<!-- Unnatural Regeneration -->

	<HediffDef ParentName="PsycastHediffBase">
		<defName>BEWH_UnnaturalRegeneration</defName>
		<label>unnatural regeneration</label>
		<description>Psychically-induced regeneration cause by a psyker of Nurgle</description>
		<hediffClass>VanillaPsycastsExpanded.Hediff_RegrowLimbs</hediffClass>
		<isBad>false</isBad>
		<stages>
			<li>
				<naturalHealingFactor>10</naturalHealingFactor>
				<statFactors>
					<InjuryHealingFactor>10</InjuryHealingFactor>
				</statFactors>
				<statOffsets>
					<Beauty>-5</Beauty>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>

	<!-- Cloud of Flies -->

	<HediffDef ParentName="PsycastHediffBase">
		<defName>BEWH_CloudOfFlies</defName>
		<label>cloud of flies</label>
		<description>Swarming flies that shields the user and attacks his enemies in range.</description>
		<initialSeverity>1</initialSeverity>
		<hediffClass>HediffWithComps</hediffClass>
		<isBad>false</isBad>
		<comps>
			<li Class="VFECore.Shields.HediffCompProperties_Shield">
				<compClass>VanillaPsycastsExpanded.Conflagrator.HediffComp_FireShield</compClass>
				<graphic>
					<texPath>Effects/Nurgle/CloudOfFliesStatic/CloudOfFliesStatic</texPath>
					<drawSize>(14, 14)</drawSize>
					<graphicClass>Graphic_Single</graphicClass>
					<shaderType>TransparentPostLight</shaderType>
				</graphic>
				<absorbAttackType>Ranged</absorbAttackType>
				<doDust>false</doDust>
				<absorbedFleck>ExplosionFlash</absorbedFleck>
				<sustainer>BEWH_NoSoundSustain</sustainer>
				<soundEnded>BEWH_NoSound</soundEnded>
			</li>
			<li Class="VFECore.Shields.HediffCompProperties_DamageAura">
				<compClass>VFECore.Shields.HediffComp_DamageAura</compClass>
				<graphic Class="VanillaPsycastsExpanded.Graphics.GraphicData_Animated">
					<texPath>Effects/Nurgle/CloudOfFlies</texPath>
					<drawSize>(14, 14)</drawSize>
					<ticksPerFrame>3</ticksPerFrame>
					<random>true</random>
					<graphicClass>VanillaPsycastsExpanded.Graphics.Graphic_Animated</graphicClass>
					<shaderType>TransparentPostLight</shaderType>
				</graphic>
				<damageDef>BEWH_CloudOfFlies</damageDef>
				<damageAmount>1</damageAmount>
				<ticksBetween>30</ticksBetween>
				<radius>6</radius>
				<hostileOnly>false</hostileOnly>
				<sustainer>BEWH_CloudOfFlies</sustainer>
				<soundEnded>BEWH_CloudOfFliesEnd</soundEnded>
			</li>
		</comps>
		<stages>
			<li>
				<statFactors>
					<IncomingDamageFactor>0.8</IncomingDamageFactor>
				</statFactors>
				<statOffsets>
					<ArmorRating_Blunt>0.55</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.55</ArmorRating_Sharp>
					<ArmorRating_Heat>0.25</ArmorRating_Heat>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>

</Defs>