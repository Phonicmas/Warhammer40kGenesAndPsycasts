<Defs>

  <!-- Psyker Path-->

  <Psyker.PsykerPathDef>
    <defName>BEWH_Psyker</defName>
    <label>sanctioned psyker</label>
    <description>Sanctioned psykers possess a rare willpower to control their abilities and not
      become a danger to others.</description>
    <background>UI/Backgrounds/PsykerPath</background>
    <altBackground>UI/AlternativeBackgrounds/PathBG_Psyker</altBackground>
    <tab>Psyker Disciplines</tab>
    <tooltip>Powerful sanctioned psyker psycasts</tooltip>
    <lockedReason>Missing Psyker Gene</lockedReason>
    <requiredGeneAny>
      <li>BEWH_Psyker</li>
      <li>BEWH_IotaPsyker</li>
      <li>BEWH_DeltaPsyker</li>
      <li>BEWH_BetaPsyker</li>
    </requiredGeneAny>
  </Psyker.PsykerPathDef>

