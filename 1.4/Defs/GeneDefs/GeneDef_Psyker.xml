        <capMods>
            <li>
                <capacity>Consciousness</capacity>
                <postFactor>1.05</postFactor>
            </li>
        </capMods>
        <marketValueFactor>2</marketValueFactor>
        <minAgeActive>6</minAgeActive>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
