  </XenotypeDef>

  <!-- Tzeentch Marine -->

  <XenotypeDef ParentName="ChaosMarineBase">
    <defName>BEWH_TzeentchMarine</defName>
    <label>tzeentch marine</label>
    <description>A once proud astartes, now led down the road of chaos, this one follows Tzeentch,
      the changer of ways.</description>
    <descriptionShort>Tzeentch follower.</descriptionShort>
    <iconPath>UI/Xenotypes/SpaceMarineTzeentchXenoIcon</iconPath>
    <genes>
      <li>BEWH_Psyker</li>
      <li>BEWH_TzeentchMark</li>
    </genes>
  </XenotypeDef>

