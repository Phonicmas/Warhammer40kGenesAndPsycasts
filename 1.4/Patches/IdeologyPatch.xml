<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Ideology</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/GeneDef[defName = "BEWH_Ossmodula"]/statOffsets</xpath>
                    <value>
                        <SuppressionPower>0.25</SuppressionPower>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/GeneDef[defName = "BEWH_Ossmodula"]/statFactors</xpath>
                    <value>
                        <SuppressionPower>1.5</SuppressionPower>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>