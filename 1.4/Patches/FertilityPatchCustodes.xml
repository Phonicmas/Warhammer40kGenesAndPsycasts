<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>False</enabled>
        <mods>
            <li>Warhammer 40k - Genes and Psycasts</li>
        </mods>
        <label>Restore custodes fertility:</label>
        <operations>
            <!-- Custodes -->
            <li Class="PatchOperationRemove">
                <xpath>Defs/GeneDef[defName = "BEWH_Custodes"]/statFactors</xpath>
            </li>
            <li Class="PatchOperationRemove">
                <xpath>Defs/GeneDef[defName = "BEWH_Custodes"]/sterilize</xpath>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/GeneDef[defName = "BEWH_Custodes"]</xpath>
                <value>
                    <statFactors>
                        <PsychicSensitivity>0</PsychicSensitivity>
                        <LeatherAmount>1.45</LeatherAmount>
                        <MeatAmount>1.45</MeatAmount>
                        <Mass>1.45</Mass>
                        <InjuryHealingFactor>12</InjuryHealingFactor>
                        <MoveSpeed>1.8</MoveSpeed>
                        <RestFallRateFactor>0.15</RestFallRateFactor>
                        <MeleeDamageFactor>4</MeleeDamageFactor>
                        <AimingDelayFactor>0.6</AimingDelayFactor>
                        <MeleeWeapon_CooldownMultiplier>0.6</MeleeWeapon_CooldownMultiplier>
                        <RangedCooldownFactor>0.6</RangedCooldownFactor>
                        <MeleeHitChance>1.8</MeleeHitChance>
                        <StaggerDurationFactor>0</StaggerDurationFactor>
                        <IncomingDamageFactor>0.55</IncomingDamageFactor>
                        <RawNutritionFactor>4</RawNutritionFactor>
                        <GlobalLearningFactor>2</GlobalLearningFactor>
                        <ImmunityGainSpeed>8</ImmunityGainSpeed>
                    </statFactors>
                </value>
            </li>
        </operations>
    </Operation>
</Patch>